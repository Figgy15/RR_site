<h2>Sign up</h2>
<div class="row">
  <section class="small-12 columns panel callout radius">
<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>
<form data-abide>
  <div class="name-field"><%= f.label :first_name %><small>required</small><br />
  <%= f.text_field :first_name%><small class="error">Name is required and must be a string.</small></div>

<div><%= f.label :last_name %><br />
  <%= f.text_field :last_name%></div>

<div><%= f.label :phone %><br />
  <%= f.text_field :phone%></div>

  <div><%= f.label :email %><br />
  <%= f.email_field :email, :autofocus => true %></div>

  <div><%= f.label :password %><br />
  <%= f.password_field :password %></div>

  <div><%= f.label :password_confirmation %><br />
  <%= f.password_field :password_confirmation %></div>
  <div><%= f.submit "Sign up", class: "button radius" %></div>
</form>
<% end %>
  </section>
</div>